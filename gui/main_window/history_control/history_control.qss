QWidget#topWidget
{
    background-color: #ffffff;
    border-style: solid;
    border-width: 1dip;
    border-color: #d7d7d7;
    border-left: none;
    border-right: none;
    border-top: none;
}

QPushButton#callButton
{
    min-width: 24dip;
    max-width: 24dip;
    min-height: 24dip;
    max-height: 24dip;
    border-image: url(:/resources/i_call_d_100.png);
}

QPushButton#callButton:hover
{
    border-image: url(:/resources/i_call_d_100_hover.png);
}

