QWidget[DateWidget="true"]
{
    min-height: 20dip;
    max-height: 20dip;
    margin-top: 12dip;
    margin-bottom: 12dip;
    border-radius: 10dip;
}

QLabel[DateWidgetLabel="true"]
{
    font-size: 12dip;
    font-family: "%FONT_FAMILY_MEDIUM%";
    font-weight: %FONT_WEIGHT_MEDIUM%;
    margin-left: 12dip;
    margin-right: 12dip;
    margin-top: 10dip;
}

QWidget[ServiceMessageOverlay="true"]
{
    min-height: 20dip;
    max-height: 20dip;
    margin-top: 12dip;
    margin-bottom: 12dip;
}

QWidget[ServiceMessageWidgetOverlay="true"]
{
    min-height: 20dip;
    max-height: 20dip;
    margin-top: 12dip;
    margin-bottom: 12dip;
    border-radius: 10dip;
}

QLabel[ServiceMessageLabelOverlay="true"]
{
    font-size: 12dip;
    font-family: "%FONT_FAMILY_MEDIUM%";
    font-weight: %FONT_WEIGHT_MEDIUM%;
    margin-left: 12dip;
    margin-right: 12dip;
    margin-top: 10dip;
}

QWidget[NewMessagesPlateCommonWidget="true"]
{
    margin-top: 12dip;
    margin-bottom: 12dip;
}

QWidget[NewMessagesPlateWidget="true"]
{
    min-height: 32dip;
    max-height: 32dip;
    margin-top: 12dip;
    margin-bottom: 12dip;
}

QLabel[NewMessagesPlateWidgetLabel="true"]
{
    font-size: 16dip;
    margin-left: 12dip;
    margin-right: 12dip;
    margin-top: 13dip;
}

QWidget#typingWidget
{
    background-color: transparent;
    min-height: 32dip;
    max-height: 32dip;
}
