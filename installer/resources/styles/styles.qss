QWidget
{
	font-family: %FONT_FAMILY%;
}

QScrollBar:vertical
{
	background-color: transparent;
	width:6dip;
}

QScrollBar::handle:vertical
{
	background-color: #C4C4C4;
	min-height: 20dip;
}

QScrollBar::add-line:vertical
{
	background-color: transparent;
}

QScrollBar::sub-line:vertical
{
	background-color: transparent;
}


QScrollBar:horizontal
{
	background-color: transparent;
	height:6dip;
}

QScrollBar::handle:horizontal
{
	background-color: #C4C4C4;
	min-width: 20dip;
}

QScrollBar::add-line:horizontal
{
	background-color: transparent;
}

QScrollBar::sub-line:horizontal
{
	background-color: transparent;
}


installer--ui--main_window
{
	background-color: white;
}

QWidget#logo_icq
{
	background-repeat: no-repeat;
	background-position: center;
	background-image: url(:/images/content_logo_100.png);
}

QWidget#logo_agent
{
	background-repeat: no-repeat;
	background-position: center;
	background-image: url(:/images/content_logo_agent_100.png);
}


QLabel#start_page_welcome_label_icq
{
	font-size: 32dip; 
}

QLabel#start_page_welcome_label_agent
{
	font-size: 28dip; 
}


QLabel#start_page_will_install
{
	color: #696969;
	font-size: 15dip; 
}

QLabel#start_page_privacy_label
{
	color: #696969;
	font-size: 13dip; 
}

QLabel#start_page_privacy_link
{
	color: #57b359;
	font-size: 13dip; 
}

QPushButton#custom_button
{
	color: #ffffff;
	font-size: 16dip;
	background-color: #57b359;
	border-style: none;
	border-radius: 4dip;
	margin: 0;
	padding-left: 20dip;
	padding-right: 20dip;
	min-width: 80dip;
	max-height: 32dip;
	min-height: 32dip;
}

QPushButton#custom_button:hover
{
	background-color: #43a047;
}

QPushButton#custom_button:disabled
{
	background-color: #cbcbcb;
}

QPushButton#frame_button
{
	color: #ffffff;
	font-size: 16dip;
	background-color: #d7d7d7;
	border-style: none;
	border-radius: 4dip;
	margin: 0;
	padding-left: 20dip;
	padding-right: 20dip;
	min-width: 80dip;
	max-height: 32dip;
	min-height: 32dip;
}

QPushButton#frame_button:disabled
{
	background-color: #cbcbcb;
}


QLabel#progress_page_demo_label
{
	font-size: 24dip;
	margin-left: 16dip;
	margin-right: 16dip;
}

QProgressBar
{
	border: none;
	margin-left: 16dip;
	margin-right: 16dip;
	background-color: #eaeae2;
}

QProgressBar::chunk
{
    background-color: #57b359;
}


QLabel#error_page_caption
{
	color: #e30f04;
	font-size: 24dip; 
}

QLabel#error_page_comment
{
	color: #696969;
	font-size: 15dip; 
}

QLabel#error_page_error_code
{
	color: #696969;
	font-size: 15dip; 
}


QLabel#error_page_support_link
{
	color: #57b359;
	font-size: 15dip; 
}

QLabel#choose_account_label
{
	font-size: 22dip; 
}

QLabel#choose_account_comment_label
{
	font-size: 15dip;
}

QLabel#label_account_nick
{
	color: #000000;
	font-size: 15dip;
}

QLabel#label_account_uin
{
	color: #696969;
	font-size: 15dip;
}

QPushButton#account_check
{
	background-image: url(:/images/check_icon_100.png);
	background-position: right;
	background-repeat: none;
	background-color: white;
	border: none;
}

installer--ui--account_widget
{
	background-image: url(:/images/check_icon_100.png);
	background-position: right center;
	background-repeat: none;
	background-color: white;
	border: none;
	margin-right: 12dip;
	border: solid;
}


installer--ui--account_widget:checked
{
	background-image: url(:/images/check_icon_100_active.png);
}

installer--ui--account_widget:hover
{
	background-image: url(:/images/check_icon_100_hover.png);
}

installer--ui--account_widget:checked:hover
{
	background-image: url(:/images/check_icon_100_active.png);
}


QWidget#agent_page_icon
{
	border: none;
	background-color: white;
	background-image: url(:/images/step_1_100_active.png);
	background-repeat: none;
}


QWidget#agent_page_icon[state="normal"]
{
	background-image: url(:/images/step_1_100_active.png);
}

QWidget#agent_page_icon[state="normalchecked"]
{
	background-image: url(:/images/step_100_active.png);	
}

QWidget#agent_page_icon[state="disabled"]
{
	background-image: url(:/images/step_100.png);	
}

QWidget#agent_page_icon[state="disabledchecked"]
{
	background-image: url(:/images/step_100.png);	
}


QLabel#agent_page_text
{
	color: red;
	border: none;
	background-color: white;	
	font-size: 14dip;
	margin-bottom: 1dip;
}

QLabel#agent_page_text[state="normal"]
{
	color: #57b359;
}

QLabel#agent_page_text[state="normalchecked"]
{
	color: #57b359;
}

QLabel#agent_page_text[state="disabled"]
{
	color: #999999;
}

QLabel#agent_page_text[state="disabledchecked"]
{
	color: #999999;
}

QLabel#agent_page_text[state="gray"]
{
	color: #999999;
}


